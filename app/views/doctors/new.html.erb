<% content_for :title, "New Doctor Hire for #{@hospital.name}" %>
<%= form_for [@hospital, @doctor] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :specialty %>
  <%= f.text_field :specialty %>

  <%= f.label :university %>
  <%= f.number_field :university %>

  <%= f.submit "Create Doctor" %>
<% end %>
